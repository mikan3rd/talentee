{
  "private": true,
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "engines": {
    "node": "14"
  },
  "scripts": {
    "frontend": "yarn workspace frontend",
    "backend": "yarn workspace backend",
    "lint": "eslint .",
    "lint:frontend": "eslint ./packages/frontend",
    "lint:backend": "eslint ./packages/backend",
    "codegen": "yarn frontend codegen && yarn eslint ./packages/frontend/graphql --fix"
  },
  "dependencies": {
    "@apollo/client": "3.11.8",
    "@prisma/client": "2.30.3",
    "@sentry/node": "6.19.7",
    "@sentry/react": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "@sentry/webpack-plugin": "1.21.0",
    "dayjs": "1.11.13",
    "firebase": "8.10.1",
    "firebase-admin": "9.12.0",
    "graphql": "15.9.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.12.0",
    "@types/node": "14.18.63",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv": "8.6.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-import-resolver-webpack": "0.13.9",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "globby": "11.1.0",
    "prettier": "2.8.8",
    "prisma": "2.30.3",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.15.0",
    "typescript": "4.9.5"
  }
