{
  "private": true,
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "engines": {
    "node": "14"
  },
  "scripts": {
    "frontend": "yarn workspace frontend",
    "backend": "yarn workspace backend",
    "lint": "eslint .",
    "lint:frontend": "eslint ./packages/frontend",
    "lint:backend": "eslint ./packages/backend",
    "codegen": "yarn frontend codegen && yarn eslint ./packages/frontend/graphql --fix"
  },
  "dependencies": {
    "@apollo/client": "3.5.5",
    "@prisma/client": "2.30.3",
    "@sentry/node": "6.15.0",
    "@sentry/react": "6.15.0",
    "@sentry/tracing": "6.15.0",
    "@sentry/webpack-plugin": "1.18.3",
    "dayjs": "1.10.7",
    "firebase": "8.10.0",
    "firebase-admin": "9.12.0",
    "graphql": "15.7.2"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.7.0",
    "@types/node": "14.18.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv": "8.6.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "globby": "11.0.4",
    "prettier": "2.5.0",
    "prisma": "2.30.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.2"
  }
}
