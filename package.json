{
  "private": true,
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "engines": {
    "node": "14"
  },
  "scripts": {
    "frontend": "yarn workspace frontend",
    "backend": "yarn workspace backend",
    "lint": "eslint .",
    "lint:frontend": "eslint ./packages/frontend",
    "lint:backend": "eslint ./packages/backend",
    "codegen": "yarn frontend codegen && yarn eslint ./packages/frontend/graphql --fix"
  },
  "dependencies": {
    "@apollo/client": "3.3.13",
    "@prisma/client": "2.20.1",
    "@sentry/node": "6.2.5",
    "@sentry/react": "6.2.5",
    "@sentry/tracing": "6.2.5",
    "@sentry/webpack-plugin": "1.14.2",
    "dayjs": "1.10.4",
    "firebase": "8.3.2",
    "firebase-admin": "9.6.0",
    "graphql": "15.5.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.2.0",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "dotenv": "8.2.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "globby": "11.0.3",
    "prettier": "2.2.1",
    "prisma": "2.20.1",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3"
  }
}
